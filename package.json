{
  "name": "va-auth-axios-adapter",
  "version": "0.0.1",
  "description": "An Axios adapter for Vue Admin authentication and authorization",
  "author": "sgobotta <santiago@camba.coop>",
  "license": "GPL-3.0",
  "main": "dist/main.js",
  "scripts": {
    "start": "npm run build && node dist/cli.js",
    "changelog": "github_changelog_generator && git add CHANGELOG.md && git commit -am \"Updates changelog\"",
    "build": "node_modules/.bin/rimraf dist/ && node_modules/.bin/babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./npm-debug.log --copy-files",
    "prepublish": "npm run build",
    "publish": "git push origin --tags && npm run changelog && git push origin",
    "release:major": "npm version major && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:patch": "npm version patch && npm publish"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint",
      "sourceType": "module"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "rimraf": "^2.6.3",
    "vue-admin-js": "^0.0.3"
  },
  "homepage": "https://github.com/Cambalab/va-auth-axios-adapter.git#readme",
  "keywords": [
    "authentication",
    "authorization",
    "vue-admin",
    "adapter",
    "npm",
    "open-source",
    "package"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cambalab/va-auth-axios-adapter.git"
  }
}
